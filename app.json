{
  "expo": {
    "name": "Anonymate",
    "slug": "Anonymate",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mainstream.anonymate"
    },
    "android": {
      "package": "com.mainstream.anonymate"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "hidden": false,
      "translucent": "false"
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    }
  }
}